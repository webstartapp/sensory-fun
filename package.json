{
  "name": "temp_init",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "knex-build": "npx tsc -p tsconfig.knex.json&& npm run knex-cleanup && npm run knex-move",
    "knex-cleanup": "rm -rf ./.next/standalone/migrations/* && rm -rf ./.next/standalone/seeds/* && rm -f ./.next/standalone/knexfile.js",
    "knex-move": "mv ./.next/standalone/dist/migrations ./.next/standalone/migrations && mv ./.next/standalone/dist/seeds ./.next/standalone/seeds && mv ./.next/standalone/dist/knexfile.js ./.next/standalone/knexfile.js && rm -rf ./.next/standalone/dist",
    "start": "next start",
    "lint": "eslint",
    "migrate": "npx knex migrate:latest",
    "seed": "npx knex seed:run"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "knex": "^3.1.0",
    "lucide-react": "^0.554.0",
    "next": "16.0.3",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.5.5",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.1",
    "resend": "^6.5.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}